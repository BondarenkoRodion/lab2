class Rational:
    def __init__(self, n=1, d=1):
        m = min(n, d)
        c = 1
        for i in range(1, m + 1):
            if n % i == 0 and d % i == 0:
                c = i
        self.n = n / c
        self.d = d / c

    def print_decimal(self):
        print(str(self.n) + '/' + str(self.d))

    def print_fraction(self):
        print(self.n / self.d)
